{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@u-wave/react-vimeo": "^0.9.5",
    "antd": "^4.18.7",
    "axios": "^0.25.0",
    "bootstrap": "^5.1.3",
    "css-minimizer-webpack-plugin": "^5.0.0",
    "depcheck": "^1.4.3",
    "env-cmd": "^10.1.0",
    "i18next": "^21.6.11",
    "i18next-browser-languagedetector": "^6.1.3",
    "i18next-http-backend": "^1.3.2",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-anchor-link-smooth-scroll": "^1.0.12",
    "react-bootstrap": "^2.1.2",
    "react-burger-menu": "^3.0.8",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.15.3",
    "react-icons": "^4.3.1",
    "react-intl": "^5.24.7",
    "react-router-dom": "^5.3.0",
    "react-scripts": "5.0.0",
    "react-scroll-to-top": "^3.0.0",
    "react-slick": "^0.29.0",
    "reactstrap": "^9.0.1",
    "sass": "^1.49.0",
    "video-react": "^0.15.0",
    "web-vitals": "^2.1.3"
  },
  "scripts": {
    "start:naked": "npm run start-js",
    "start": "env-cmd -f ./environments/.env.development npm run start:naked",
    "start:development": "env-cmd -f ./environments/.env.development npm run start:naked",
    "start:staging": "env-cmd -f ./environments/.env.staging npm run start:naked",
    "start:apstaging": "env-cmd -f ./environments/.env.apstaging npm run start:naked",
    "start:prod": "env-cmd -f ./environments/.env.production npm run start:naked",
    "start:testing": "env-cmd -f ./environments/.env.testing npm run start:naked",
    "start:local": "env-cmd -f ./environments/.env.local npm run start:naked",
    "start-js": "react-scripts start",
    "build": "react-scripts build",
    "build:staging": "env-cmd -f ./environments/.env.staging npm run build",
    "build:apstaging": "env-cmd -f ./environments/.env.apstaging npm run build",
    "build:prod": "env-cmd -f ./environments/.env.production npm run build",
    "build:development": "env-cmd -f ./environments/.env.development npm run build",
    "build:testing": "env-cmd -f ./environments/.env.testing npm run build",
    "build:local": "env-cmd -f ./environments/.env.local npm run build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "lint": "eslint src/**/*.js",
    "lint-fix": "eslint src/**/*.js --fix"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-syntax-jsx": "^7.16.7",
    "@babel/preset-react": "^7.16.7",
    "@storybook/addon-actions": "^6.4.17",
    "@storybook/addon-essentials": "^6.4.17",
    "@storybook/addon-links": "^6.4.17",
    "@storybook/builder-webpack5": "^6.4.17",
    "@storybook/manager-webpack5": "^6.4.17",
    "@storybook/node-logger": "^6.4.18",
    "@storybook/preset-create-react-app": "^4.0.0",
    "@storybook/react": "^6.4.17",
    "@storybook/testing-react": "^1.2.3",
    "@testing-library/react": "^12.1.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^8.22.0",
    "eslint-plugin-react": "^7.30.1",
    "react-test-renderer": "^17.0.2",
    "webpack": "^5.68.0"
  }
}
